{
  "manifest_version": 3,
  "name": "AlwaysDefaultContextMenu",
  "version": "0.1",
  "description": "Blocks disabling of context menu and text selection on websites.",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
